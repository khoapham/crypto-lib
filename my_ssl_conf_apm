openssl_conf = openssl_def

[openssl_def]
engines = engine_section

[engine_section]
apm = apm_section

[apm_section]
engine_id = apm
dynamic_path = /usr/lib/engines/libapm.so
default_algorithms = ALL
CIPHERS = aes-128-cbc aes-192-cbc aes-256-cbc des-cbc
DIGESTS = md4 md5 sha1 sha224 sha256 sha512
init = 0
